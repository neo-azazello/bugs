<div class="modal fade" id="addProject" tabindex="-1" data-keyboard="false" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">Add new project</h5>
         <button type="button" class="close" data-dismiss="modal" aria-label="Close"> </button>
      </div>
      <form action="{{ path_for('addproject') }}" method="post" autocomplete="off" enctype="multipart/form-data">
      <div class="modal-body">
        
        <div class="form-group">
            <label class="form-label">Project Name</label>
            <input type="text" class="form-control" name="projectname" placeholder="Text..">
        </div>
        
        <div class="form-group">
          <label class="form-label">Project start date</label>
          <input type="text" name="projectsartdate" class="form-control" data-mask="00.00.0000" data-mask-clearifnotmatch="true" placeholder="00.00.0000" autocomplete="off" maxlength="10">
        </div>
        
        <div class="form-group">
            <label>Project logo</label>
            <input type="file" class="form-control" id="profile_file">
        </div>
        
        <input type="hidden" name="projectlogo" id="photo" value="" />
      
      </div>
      <div class="modal-footer">
        <button type="submit" class="btn btn-primary">Save changes</button>
      </div>
      </form>
      <script>
        require(['input-mask']);
      </script>
    </div>
  </div>
</div>